# Personal GitHub account 
Host github
  HostName github.com
  User git 
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519_github

# GitLab account (DulinLab)
Host gitlab-dulinlab
  HostName gitlab.com 
  User git 
  PreferredAuthentications publickey
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519_dulinlab_gitlab

